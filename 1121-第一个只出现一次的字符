class Solution {
public:
    char firstUniqChar(string s) {
        map<char, int> m;
        for(int i=0; i<s.size(); i++){
            if(m.find(s[i])!=m.end()) m[s[i]]+=1;
            else m[s[i]]=1;
        }
        for(int i=0; i<s.size(); i++){
            if(m[s[i]]==1) return s[i];
        }
        return ' ';
    }
};
